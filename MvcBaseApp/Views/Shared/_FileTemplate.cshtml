@model DataModel.Document

<script>
    function onDocumentLookUpClick(s, e) {
        switch(e.buttonIndex) {
            case 1:
                $('#fileWorker_document').click();
                break;
            case 2:
                clearFile();
                break;
        }
    }
    //function onWorkerDocumentClick(e) {
    //    $('#fileWorker_document').click();
    //}

    function onFileSelected() {
        document.getElementById('Document.body_I').value = $('#fileWorker_document').val();
        $('#document_Id').val(0);
        //$('#IsDirty').val('false');
    }

    function clearFile() {
        $('#document_Id').val(0);
        $('#fileWorker_document').val('');
        document.getElementById('Document.body_I').value = $('#fileWorker_document').val();
    }

    var str = '@Model.Filename';
    if (str!='')
        document.getElementById('Document.body_I').value = 'C:\\fakepath\\'+str;
</script>


<input type="file" style="display: none;" name="fileWorker_document" onchange="onFileSelected()" id="fileWorker_document" value="" />

