@using MvcBaseApp.Models
@{
    Layout = null;
    var id = ((int?) ViewBag.Message_Id_Request ?? 0);
}
    @Html.DevExpress().CallbackPanel(s =>
{
    s.Name = "pnl_Messages";
    s.CallbackRouteValues = new { Controller = "Message", Action = "pnl_LicenseMessagesPartial" };

    s.SetContent(() =>
    {
        Html.RenderAction("LicenseMessagesPartial" , new { Id_Request = id});
    });
}).GetHtml()